{
    "name": "odama/core",
    "description": "Odama - Laravel Core Library for rapid development and security",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Odama Team",
            "email": "support@odama.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.0|^12.0",
        "bacon/bacon-qr-code": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Odama\\": "src/app/"
        },
        "files": [
            "helpers/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Odama\\Providers\\OdamaServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
